{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "jasmine": true
  },
  "plugins": [],
  "rules": {
    "max-len": [
      2,
      120
    ],
    "prefer-arrow-callback": 0,
    "arrow-parens": [
      1
    ],
    "keyword-spacing": 1,
    "space-before-blocks": [1],
    "quotes": 0,
    "no-trailing-spaces": 1,
    "comma-dangle": 0,
    "comma-spacing": 0,
    "padded-blocks": 0,
    "no-unused-vars": 1,
    "no-mixed-operators": 1,
    "no-continue": 1,
    "no-lonely-if": 1,
    "no-param-reassign": 1,
    "no-use-before-define": 0,
  }
}
